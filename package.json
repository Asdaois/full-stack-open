  "scripts": {
    "start": "cd server && npm install && npm run start",
    "dev": "concurrently npm:dev:* -c \"blue,green\"",
    "dev:client": "cd client && npm run start",
    "dev:server": "cd server && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:ui": "rm -rf server/build && cd ./client && npm run build ",
    "deploy": "git push origin release",
    "deploy:full": "git checkout release && npm run build:ui  && git add . && git commit -m uibuild && git push && npm run deploy",    
    "logs:prod": "heroku logs --tail"
  },
